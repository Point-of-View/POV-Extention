{
    "name": "Point of View",
    "version": "1.0.0",
    "description": "Converting news articles to highlight opposing viewpoints",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ],
    "action":{
        "default_popup": "index.html",
        "default_title": "Point of View"
    }
}